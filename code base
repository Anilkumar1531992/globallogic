fun main(args: Array<String>) {
    val a = intArrayOf(1, 2, 6, 3, 4, 5)
    val b = intArrayOf(2, 4, 3, 1, 0)
    val n = a.size
    val m = b.size
    findMissing(a, b, n, m)
}

fun findMissing(a: IntArray, b: IntArray, n: Int, m: Int) {
    val s = HashSet<Int>()
    for (i in 0 until m)
        s.add(b[i])
    for (i in 0 until n)
        if (!s.contains(a[i]))
            print("${a[i]} ")
}
